  conversationText:
    stringRef: {fileID: 0}
    stringVal: "You you: I really like this costume, can I try it on?\r\nShopkeeper:
      Sure, the fitting room is over here.\r\nYou you: Step out of the fitting room.\r\nShopkeeper
